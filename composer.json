{
  "name": "mdarmancse/autolara",
  "description": "A Laravel package for auto-generating CRUD operations.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Mdarmancse\\AutoLara\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Md Arman",
      "email": "mdarmancse@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^11.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../autolara"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Mdarmancse\\AutoLara\\AutoLaraServiceProvider"
      ]
    }
  }
}
